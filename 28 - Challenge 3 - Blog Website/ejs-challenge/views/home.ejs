
<%- include("partials/header") %>
<h1>Home</h1>

<p> <%= startingContent %> </p>

<% posts.forEach(function(post) { %>
    <h2> <%= post.title %></h2>
    <p><%= post.content.substring(0, 100) %>...</p>
    <a href="/posts/<%= _.lowerCase(post.title) %>">Read More</a>
    <% console.log(post.title); %>
<% }); %>

<%- include("partials/footer") %>